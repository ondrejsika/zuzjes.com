{
  "version": 2,
  "name": "zuzjes",
  "alias": "zuzjes.com",
  "builds": [
    {
      "src": "package.json",
      "use": "@now/static-build",
      "config": { "distDir": "out" }
    }
  ],
  "routes": [
    {
      "src": "/resume.pdf",
      "status": 302,
      "headers": {
        "Location": "/static/resume-zuzana-jeschke.pdf"
      }
    },
    {
      "src": "/.well-known/security.txt",
      "dest": "/static/security.txt"
    },
    {
      "src": "/security.txt",
      "status": 301,
      "headers": {
        "Location": "/.well-known/security.txt"
      }
    }
  ]
}
